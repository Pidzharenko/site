define(["jquery","underscore","utils"],function(e,n,t){var s,i={},r=[],a=e('<ul class="apps stacked"></ul>');return i.$el=a,i.setResource=function(e){return s=e,r=e?e["applications-used"]:null,i},i.render=function(){return r&&r.length&&r.length>0&&(a.empty(),n.each(r,function(e){var n=e.name,s=t.appCodeMap[n],i=(e.versions,e.versions[0]),r=t.supportsRetinaDisplay?"_2x":"",l=t.getProductIconPath();null!==s&&void 0!==s&&a.append('<li title="'+n+(i?i.version?" "+i.version:"":"")+'" class="app_list_item"><a href="'+t.goURL("http://www.adobe.com/go/"+s)+'"><img src="'+l+"/"+s+"_small"+r+'.png"></img>'+n+"</a></li>")})),i},i});